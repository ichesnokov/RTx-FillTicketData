<%init>
use RTx::AutofillCustomFields;
my $config = RTx::AutofillCustomFields::config();
if (    $config
     && $CustomField
     && $config->{key_fields}->{ $CustomField->Id } ) {
    $m->out(   q{<td class="entry"><button id='autofill_custom_fields'>}
             . loc('Get data')
             . q{</button></td>} );
}
</%init>
<%ARGS>
$CustomField => undef
</%ARGS>
